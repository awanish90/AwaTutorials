<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:height="100dp"
        android:width="50dp">
        <shape android:shape="rectangle">
            <solid android:color="@android:color/transparent" />
        </shape>
    </item>
   <!-- <item
        >
        <shape>
            <size android:width="5dp" />
            <stroke android:color="@color/app_color" />
        </shape>
    </item>-->

     <item  android:bottom="1dp"
         android:top="1dp"
         android:right="20dp"
         android:left="20dp">

        <rotate android:fromDegrees="90">
        <shape android:shape="line">
            <size android:width="5dp"
                android:height="45dp"/>
            <stroke android:width="5dp"
                android:color="@color/app_color"/>
                />
            <padding
                android:left="5dp"
                android:right="5dp"
                android:top="1dp"
                android:bottom="1dp"/>
        </shape>
        </rotate>
    </item>
    <item
        android:bottom="45dp"
        android:top="70dp"
        android:right="20dp"
        android:left="20dp">
        <shape android:shape="oval">
            <size
                android:width="10dp"
                android:height="10dp" />
            <solid android:color="@color/orange_camera" />
        </shape>
    </item>

 <item
     android:bottom="15dp"
     android:top="55dp"
     android:right="20dp"
     android:left="20dp">
        <rotate android:fromDegrees="0">
            <shape android:shape="line">
                <stroke android:width="5dp"
                    android:color="@color/app_color"/>
                />
                <padding
                    android:left="5dp"
                    android:right="5dp"
                    android:top="1dp"/>
            </shape>
        </rotate>
    </item>
